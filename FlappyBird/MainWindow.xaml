<Window x:Class="FlappyBird.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlappyBird"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

	<Grid>
		<Canvas Name="MyCanvas" Focusable="True" KeyDown="KeyIsDown" KeyUp="KeyIsUp" Background="SkyBlue"  Width="800" Height="450">
		

		<Image Height="390" Width="66" Source="/pipeBottom.png" Tag="obs1" Canvas.Left="106" Canvas.Top="262"/>
		<Image Height="390" Width="66" Source="/pipeTop.png" Tag="obs1" Canvas.Left="106" Canvas.Top="-270"/>

			<Image Height="390" Width="66" Source="/pipeBottom.png" Tag="obs1" Canvas.Left="310" Canvas.Top="239"/>
			<Image Height="390" Width="66" Source="/pipeTop.png" Tag="obs1" Canvas.Left="310" Canvas.Top="-292"/>

			<Image Height="390" Width="66" Source="/pipeBottom.png" Tag="obs1" Canvas.Left="522" Canvas.Top="320"/>
			<Image Height="390" Width="66" Source="/pipeTop.png" Tag="obs1" Canvas.Left="522" Canvas.Top="-212"/>

			<Image Height="390" Width="66" Source="/pipeBottom.png" Tag="obs1" Canvas.Left="714" Canvas.Top="283"/>
			<Image Height="390" Width="66" Source="/pipeTop.png" Tag="obs1" Canvas.Left="714" Canvas.Top="-249"/>

			<Image Name="madar" Source="/flappyBird.png" Height="36" Width="50" Stretch="Fill" Canvas.Left="0" Canvas.Top="190"  Panel.ZIndex="100"/>

		<Label Name="lbl_Score" FontSize="22" FontWeight="ExtraBold" Content="Score :0" Canvas.Left="10" Canvas.Top="10"/>

		</Canvas>
	</Grid>
</Window>
